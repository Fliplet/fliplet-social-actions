{
  "name": "Social actions",
  "package": "com.fliplet.social-icons",
  "version": "1.0.0",
  "icon": "img/icon.png",
  "tags": ["type:component", "category:general", "beta:true"],
  "provider_only": false,
  "references": [],
  "settings": {
    "asyncRendering": true,
    "interfaceWidgetPackage": "com.fliplet.helper-configuration"
  },
  "html_tag": "div",
  "interface": {
    "dependencies": [
      "fliplet-core",
      "fliplet-studio-ui",
      "fliplet-datasources"
    ],
    "assets": [
      "js/interface.js"
    ]
  },
  "build": {
    "dependencies": [
      "jquery",
      "bootstrap",
      "fliplet-core",
      "fliplet-helper",
      "fliplet-datasources"
    ],
    "assets": [
      "css/index.scss",
      "css/build.css",
      "js/build.js"
    ],
    "appAssets": []
  },
  "themeSettings": {
    "variablePrefix": "social-icons",
    "packages": ["com.fliplet.social-icons"],
    "name": "Social actions",
    "appSupportsContainers": true,
    "variables": [
      {
        "description": "Color",
        "fields": [
          {
            "name": "background",
            "default": "#ccc",
            "label": "Full icon color",
            "type": "color",
            "breakpoints": {
              "tablet": {
                "name": "backgroundTablet",
                "default": "inherit-mobile"
              },
              "desktop": {
                "name": "backgroundDesktop",
                "default": "inherit-tablet"
              }
            }
          },
          {
            "name": "backgrounddd",
            "default": "#ccc",
            "label": "Full icon color",
            "type": "color",
            "breakpoints": {
              "tablet": {
                "name": "backgroundTablettt",
                "default": "inherit-mobile"
              },
              "desktop": {
                "name": "backgroundDesktoppp",
                "default": "inherit-tablet"
              }
            }
          }
        ]
      }
    ]
  }
}
